{
	"rules": {
		"no-unused-expression-chai": true,
		"no-duplicate-variable": true,
		"no-unused-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			"always"
		],
		"triple-equals": true,
		"no-var-keyword": true,
		"forin": false,
		"no-empty": true,
		"no-eval": true,
		"no-null-keyword": false,
		"member-access": [
			true
		],
		"arrow-parens": true,
		"indent": [
			true
		],
		"switch-default": true,
		"whitespace": [
			true
		]
	},
	"rulesDirectory": [
		"node_modules/tslint-no-unused-expression-chai/rules"
	]
}